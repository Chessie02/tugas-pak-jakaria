#include<iostream>

using namespace std;

int main()
{
	int luas,panjang,lebar;
	cout<<"masukan nilai panjang=";
	cin>>panjang;
	cout<<"masukan nilai lebar=";
	cin>>lebar;
	luas=panjang*lebar;
	cout<<"luas persegi panjang="<<luas<<endl;
	return 0;
}